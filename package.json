{
  "name": "react-webpack-babel-kit-2",
  "version": "1.0.0",
  "description": "React Webpack 2.0 babel kit with bootstrap framework",
  "main": "index.html",
  "scripts": {
    "start": "webpack-dev-server --progress --colors --hot --inline",
    "start:dev": "webpack-dev-server --config ./webpack.dev.config.js  --progress --colors",
    "build:dev": "webpack --config ./webpack.dev.config.js --progress --colors",
    "build": "webpack -p --config ./webpack.prod.config.js --progress --colors"
  },
  "author": "lokeam",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "bootstrap-sass": "3.3.7",
    "css-hot-loader": "1.1.1",
    "css-loader": "0.28.4",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.2",
    "node-sass": "4.5.3",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-hot-loader": "3.0.0-beta.7",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "0.5.8",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-preset-stage-1": "^6.1.18",
    "file-system": "^2.2.2",
    "lodash": "^3.10.1",
    "node-sass": "^4.5.3",
    "prop-types": "^15.6.0",
    "react": "^0.14.9",
    "react-addons-transition-group": "^15.6.0",
    "react-dom": "^0.14.9",
    "react-redux": "4.3.0",
    "react-router": "^2.0.1",
    "react-transition-group": "^1.2.0",
    "redux": "^3.0.4",
    "redux-promise": "^0.5.3"
  }
}
